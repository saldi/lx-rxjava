{
  "configuration": {
    "status": "error",
    "name": "JSONConfigDemo",
    "packages": "com.crif",
    "ThresholdFilter": {
      "level": "debug"
    },
    "appenders": {
      "Console": {
        "name": "Console",
        "PatternLayout": {
          "pattern": "%d [%t] %-5p %c - %m%n"
        }
      }
    },
    "loggers": {
      "logger": {
        "name": "edu",
        "level": "info",
        "additivity": "false",
        "AppenderRef": {
          "ref": "Console"
        }
      },
      "root": {
        "level": "info",
        "AppenderRef": {
          "ref": "Console"
        }
      }
    }
  }
}